<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AbpCommunityTalks.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AbpCommunityTalks.Maui"
    xmlns:pages="clr-namespace:AbpCommunityTalks.Maui.Pages"
    Shell.FlyoutBehavior="Disabled">


    <TabBar>
        <Tab Title="Users">
            <ShellContent
                Title="Home"
                ContentTemplate="{DataTemplate pages:IdentityUsersPage}"
                Route="MainPage" />
        </Tab>
        <Tab Title="Roles">
            <ShellContent 
                Title="FileSystem"
                ContentTemplate="{DataTemplate pages:BlankPage}"
                Route="FileSystem"/>
        </Tab>
    </TabBar>

</Shell>
